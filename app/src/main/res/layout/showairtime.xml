<?xml version="1.0" encoding="utf-8"?>


<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1818dp"
            tools:context="com.example.itmanager.MobileMoney.ShowAirtime"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="81dp">

            <TextView
                android:id="@+id/txthead"
                android:layout_width="406dp"
                android:layout_height="58dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="0dp"
                android:layout_marginBottom="1752dp"
                android:background="@color/colorPrimaryDark"
                android:gravity="center"
                android:text="Show Transaction "
                android:textColor="@color/design_default_color_background"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/resultSet"
                android:layout_width="398dp"
                android:layout_height="623dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="69dp"
                android:layout_marginRight="2dp"
                android:layout_marginBottom="1126dp" />

        </RelativeLayout>

    </LinearLayout>

</ScrollView>

